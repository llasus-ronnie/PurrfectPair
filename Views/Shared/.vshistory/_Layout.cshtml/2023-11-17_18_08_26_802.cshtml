    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - PurrfectPair - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
